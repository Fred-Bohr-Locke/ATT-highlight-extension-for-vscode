{
    "publisher": "FredBohrLocke",
	"icon": "logo_transparent.png",
    "name": "att",
    "displayName": "AT&T",
    "description": "a highlight extension for AT&T assembly language",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "keywords": [
        "AT&T", "ATT", "att", "linux", "assembly language", "highlight"
    ],
    "contributes": {
        "languages": [{
            "id": "att",
            "aliases": ["AT&T Assembly Language", "att"],
            "extensions": [".s"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "att",
            "scopeName": "source.s",
            "path": "./syntaxes/att.tmLanguage.json"
        }],
        "snippets": [{
            "language": "att",
            "path": "./snippets/snippets.code-snippets"
        }]
    }
}